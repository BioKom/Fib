					</point>
				</property>
			</area>
			<property>
				<vector type="property.colorSW">
					<value number="1">10</value>
				</vector>
				<point>
					<vector type="position">
						<value number="1">0</value>
						<value number="2">0</value>
					</vector>
				</point>
			</property>
		</list>
	</main_fib_object>
