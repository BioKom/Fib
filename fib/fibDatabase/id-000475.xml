<?xml version="1.0" encoding="UTF-8"?>
<fib_object
			  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns="http://www.fib-development.org/"
			  xsd:schemaLocation="http://www.fib-development.org/fib.xsd">
	<root>
		<multimedia_info fib_version="3" db_version="2"/>
		<optionalpart>
			<pair key="name" value="quadrangle with antialising"/>
			<pair key="description" value="This object represents a quadrangle (box) with antialising. In the first dimension it goes from start point x_s to end point x_e (borders not antialiased). In the second dimension the area is filled between two lines, which are antialiased. The first line (y_s = f_1( x ) = a_0 + a_1 * x) goes through the first two points ( x_1, y_1 ) and ( x_2, y_2 ). The second line (y_e = f_2( x ) = b_0 + b_1 * x) goes through the last two points ( x_3, y_3 ) and ( x_4, y_4 ). The start point x_s is max( min( x_1, x_2 ), min( x_3, x_4 ) ) and the end point x_e is ( min( max( x_1, x_2 ), max( x_3, x_4 ) ) - 1 ) (the '- 1' at the end is, because for antialising the pixle x goes till (x + 1) ). If x_s is bigger than x_e no box is drawn. The points of the quadrangle (box) are given back as variables of the subobject."/>
			<pair key="inVar1::description" value="position of the first point in dimension 1 (x_1)"/>
			<pair key="inVar2::description" value="position of the first point in dimension 2 (y_1)"/>
			<pair key="inVar3::description" value="position of the second point in dimension 1 (x_2)"/>
			<pair key="inVar4::description" value="position of the second point in dimension 2 (y_2)"/>
			<pair key="inVar5::description" value="position of the 3. point in dimension 1 (x_3)"/>
			<pair key="inVar6::description" value="position of the 3. point in dimension 2 (y_3)"/>
			<pair key="inVar7::description" value="position of the 4. point in dimension 1 (x_4)"/>
			<pair key="inVar8::description" value="position of the 4. point in dimension 2 (y_4)"/>
			<pair key="subObj1::description" value="The points of the quadrangle (box). The subobject is called for every point on the quadrangle (box)."/>
			<pair key="subObj1::outVar1::description" value="the position of the quadrangle point in dimension 1 (x)"/>
			<pair key="subObj1::outVar2::description" value="the position of the quadrangle point in dimension 2 (y)"/>
			<pair key="subObj1::outVar3::description" value="the antialising factor (1 is full transparent and 0 full opaque)"/>
			<pair key="author" value="Betti Oesterholz"/>
			<pair key="author::email" value="Oesterholz@Fib-Development.org"/>
			<pair key="copyright" value="LGPL3"/>
		</optionalpart>
		<input_variables>
			<variable number="1" default="0"/>
			<variable number="2" default="0"/>
			<variable number="3" default="0"/>
			<variable number="4" default="0"/>
			<variable number="5" default="0"/>
			<variable number="6" default="0"/>
			<variable number="7" default="0"/>
			<variable number="8" default="0"/>
		</input_variables>
		<domains>
			<inVar number="1">
				<domainReference subdomain="1">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="2">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="3">
				<domainReference subdomain="1">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="4">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="5">
				<domainReference subdomain="1">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="6">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="7">
				<domainReference subdomain="1">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="8">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<externSubobject number="1">
				<vector elements="2">
					<!-- actually any numbers are possible;
						the next lines are just a space holder -->
					<naturalNumberB bit="0" />
					<naturalNumberB bit="0" />
					<naturalNumberB bit="0" />
				</vector>
			</externSubobject>
			<subfunction>
				<naturalNumberB bit="12" />
			</subfunction>
			<externObject>
				<vector elements="4">
					<integer min="-465" max="-465" /><!--Identifier-->
					<integer min="6" max="6" /><!--number input values-->
					<naturalNumber max="1" /><!--number subobjects-->
					<naturalNumber max="3" /><!--number output values-->
				</vector>
			</externObject>
			<externObjectInput identifier="-465">
				<vector elements="6">
					<naturalNumber max="0" />
					<naturalNumber max="0" />
					<naturalNumber max="0" />
					<naturalNumber max="0" />
					<naturalNumber max="0" />
					<naturalNumber max="0" />
				</vector>
			</externObjectInput>
		</domains>
		<database_identifiers>
			<identifier>-465</identifier>
		</database_identifiers>
		<main_fib_object>
			<!-- evalue x_s and x_e-->
			<function define_variable="9">
				<!-- x_s = max( min( x_1, x_2 ), min( x_3, x_4 ) ) -->
				<subfunction>
					<max>
						<min>
							<variable>1</variable>
							<variable>3</variable>
						</min>
						<min>
							<variable>5</variable>
							<variable>7</variable>
						</min>
					</max>
				</subfunction>
				<function define_variable="10">
					<!-- x_e = min( max( x_1, x_2 ), max( x_3, x_4 ) ) - 1 -->
					<subfunction>
						<sub>
							<min>
								<max>
									<variable>1</variable>
									<variable>3</variable>
								</max>
								<max>
									<variable>5</variable>
									<variable>7</variable>
								</max>
							</min>
							<value>1</value>
						</sub>
					</subfunction>
					<if><!--if x_e < x_s than do nothing-->
						<condition>
							<lo>
								<variable>10</variable>
								<variable>9</variable>
							</lo>
						</condition>
						<point/>
					<!-- else x_s <= x_e evalue parameters of the two lines -->
						<if><!-- if x_1 == x_2 then connect points 1 and 2 (draw line
							from y_1 to y_2) and connect to second line f_2 -->
							<condition>
								<eqInt>
									<mult>
										<variable>1</variable>
										<value>1024</value>
									</mult>
									<mult>
										<variable>3</variable>
										<value>1024</value>
									</mult>
								</eqInt>
							</condition>
							<list>
								<!--draw line from y_1 to y_2-->
								<area define_variable="11">
									<vector type="subarea">
										<variable>2</variable>
										<variable>4</variable>
									</vector>
									<subobject number="1">
										<vector type="externSubobject">
											<variable>1</variable>
											<variable>11</variable>
											<value>0</value><!-- full opaque -->
										</vector>
									</subobject>
								</area>
								<if><!-- if x_3 == x_4 then connect points 3 and 4 (draw line
									from y_3 to y_4) and points 1 and 4 (draw line from y_1 to y_4)-->
									<condition>
										<eqInt>
											<mult>
												<variable>5</variable>
												<value>1024</value>
											</mult>
											<mult>
												<variable>7</variable>
												<value>1024</value>
											</mult>
										</eqInt>
									</condition>
									<!-- connect points 3 and 4 (draw line from y_3 to y_4 and points 1 and 4 (draw line from y_1 to y_4-->
									<list>
										<area define_variable="11">
											<vector type="subarea">
												<variable>6</variable>
												<variable>8</variable>
											</vector>
											<subobject number="1">
												<vector type="externSubobject">
													<variable>5</variable>
													<variable>11</variable>
													<value>0</value><!-- full opaque -->
												</vector>
											</subobject>
										</area>
										<area define_variable="11">
											<vector type="subarea">
												<variable>2</variable>
												<variable>8</variable>
											</vector>
											<subobject number="1">
												<vector type="externSubobject">
													<variable>1</variable>
													<variable>11</variable>
													<value>0</value><!-- full opaque -->
												</vector>
											</subobject>
										</area>
									</list>
									<!-- else x_1 == x_2 and x_3 != x_4 -> evalue parameters for line f_2 -->
									<function define_variable="11">
										<!-- b_1 slope of the line f_1 -->
										<subfunction>
											<!-- b_1 = (y_4 - y_3) / (x_4 - x_3) -->
											<div>
												<sub>
													<variable>8</variable>
													<variable>6</variable>
												</sub>
												<sub>
													<variable>7</variable>
													<variable>5</variable>
												</sub>
											</div>
										</subfunction>
										<function define_variable="12">
											<!-- b_0 = y_3 - b_1 * x_3 -->
											<subfunction>
												<sub>
													<variable>6</variable>
													<mult>
														<variable>11</variable>
														<variable>5</variable>
													</mult>
												</sub>
											</subfunction>
											<!-- draw line from f_1=y_1 to f_2 -->
											<obj identifier="-465">
												<vector type="externObjectInput">
													<variable>9</variable>
													<variable>10</variable>
													<variable>2</variable>
													<value>0</value>
													<variable>12</variable>
													<variable>11</variable>
												</vector>
												<subobject>
													<output_variables>
														<variable>13</variable>
														<variable>14</variable>
														<variable>15</variable>
													</output_variables>
													<subobject number="1">
														<vector type="externSubobject">
															<variable>13</variable>
															<variable>14</variable>
															<variable>15</variable>
														</vector>
													</subobject>
												</subobject>
											</obj>
										</function>
									</function>
								</if>
							</list>
						<!-- else x_1 != x_2 -> evalue parameters for line f_1 -->
							<function define_variable="11">
								<!-- a_1 slope of the line f_1 -->
								<subfunction>
									<!-- a_1 = (y_2 - y_1) / (x_2 - x_1) -->
									<div>
										<sub>
											<variable>4</variable>
											<variable>2</variable>
										</sub>
										<sub>
											<variable>3</variable>
											<variable>1</variable>
										</sub>
									</div>
								</subfunction>
								<function define_variable="12">
									<!-- a_0 = y_1 - a_1 * x_1 -->
									<subfunction>
										<sub>
											<variable>2</variable>
											<mult>
												<variable>11</variable>
												<variable>1</variable>
											</mult>
										</sub>
									</subfunction>
									<if><!-- if x_3 == x_4 then connect points 3 and 4 (draw line
										from y_3 to y_4) and connect to first line f_1 -->
										<condition>
											<eqInt>
												<mult>
													<variable>5</variable>
													<value>1024</value>
												</mult>
												<mult>
													<variable>7</variable>
													<value>1024</value>
												</mult>
											</eqInt>
										</condition>
										<list><!-- connect points 3 and 4 (draw line from y_3 to y_4) -->
											<area define_variable="13">
												<vector type="subarea">
													<variable>6</variable>
													<variable>8</variable>
												</vector>
												<subobject number="1">
													<vector type="externSubobject">
														<variable>5</variable>
														<variable>13</variable>
														<value>0</value><!-- full opaque -->
													</vector>
												</subobject>
											</area>
											<!-- draw line from f_2=y_3 to f_1 -->
											<obj identifier="-465">
												<vector type="externObjectInput">
													<variable>9</variable>
													<variable>10</variable>
													<variable>12</variable>
													<variable>11</variable>
													<variable>6</variable>
													<value>0</value>
												</vector>
												<subobject>
													<output_variables>
														<variable>13</variable>
														<variable>14</variable>
														<variable>15</variable>
													</output_variables>
													<subobject number="1">
														<vector type="externSubobject">
															<variable>13</variable>
															<variable>14</variable>
															<variable>15</variable>
														</vector>
													</subobject>
												</subobject>
											</obj>
										</list>
									<!-- else x_1 != x_2 and x_3 != x_4 -> evalue parameters for line f_2 -->
										<function define_variable="13">
											<!--b_1 slope of the line f_1-->
											<subfunction>
												<!-- b_1 = (y_4 - y_3) / (x_4 - x_3) -->
												<div>
													<sub>
														<variable>8</variable>
														<variable>6</variable>
													</sub>
													<sub>
														<variable>7</variable>
														<variable>5</variable>
													</sub>
												</div>
											</subfunction>
											<function define_variable="14">
												<!-- b_0 = y_3 - b_1 * x_3 -->
												<subfunction>
													<sub>
														<variable>6</variable>
														<mult>
															<variable>13</variable>
															<variable>5</variable>
														</mult>
													</sub>
												</subfunction>
												<!-- draw line from f_1=x_1 to f_2 -->
												<obj identifier="-465">
													<vector type="externObjectInput">
														<variable>9</variable>
														<variable>10</variable>
														<variable>12</variable>
														<variable>11</variable>
														<variable>14</variable>
														<variable>13</variable>
													</vector>
													<subobject>
														<output_variables>
															<variable>15</variable>
															<variable>16</variable>
															<variable>17</variable>
														</output_variables>
														<subobject number="1">
															<vector type="externSubobject">
																<variable>15</variable>
																<variable>16</variable>
																<variable>17</variable>
															</vector>
														</subobject>
													</subobject>
												</obj>
											</function>
										</function>
									</if>
								</function>
							</function>
						</if>
					</if>
				</function>
			</function>
		</main_fib_object>
	</root>
</fib_object>