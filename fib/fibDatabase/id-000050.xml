<?xml version="1.0" encoding="UTF-8"?>
<fib_object
			  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns="http://www.fib-development.org/"
			  xsd:schemaLocation="http://www.fib-development.org/fib.xsd">
	<root>
		<multimedia_info fib_version="3" db_version="2"/>
		<optionalpart>
			<pair key="name" value="circle"/>
			<pair key="description" value="This object represents a filled circle with the middle point (x, y) and radius r."/>
			<pair key="inVar1::description" value="position of the middle point in dimension 1 (x)"/>
			<pair key="inVar2::description" value="position of the middle point in dimension 2 (y)"/>
			<pair key="inVar3::description" value="size of the radius r"/>
			<pair key="author" value="Betti Oesterholz"/>
			<pair key="author::email" value="Oesterholz@Fib-Development.org"/>
			<pair key="copyright" value="LGPL3"/>
		</optionalpart>
		<input_variables>
			<variable number="1" default="0"/>
			<variable number="2" default="0"/>
			<variable number="3" default="1"/>
		</input_variables>
		<domains>
			<inVar number="1">
				<domainReference subdomain="1">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="2">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<area>
				<vector elements="2">
					<naturalNumber max="1" />
					<vector elements="2">
						<naturalNumber max="0" />
						<naturalNumber max="0" />
					</vector>
				</vector>
			</area>
			<subfunction>
				<naturalNumber scalingfactor="0.5" max="4"/>
			</subfunction>
		</domains>
		<main_fib_object>
			<if>
				<condition><!-- if radius is negativ -> do nothing -->
					<lo>
						<variable>3</variable>
						<value>0</value>
					</lo>
				</condition>
				<point/>
				<!-- else draw circle -->
				<area define_variable="4"><!-- for a -->
					<vector type="subarea">
						<value>0</value>
						<variable>3</variable>
					</vector>
					<function define_variable="5">
						<!-- b = r^2 - a^2 -->
						<subfunction>
							<sub>
								<exp>
									<variable>3</variable>
									<value>2</value>
								</exp>
								<exp>
									<variable>4</variable>
									<value>2</value>
								</exp>
							</sub>
						</subfunction>
						<function define_variable="6">
							<!-- b = ( r^2 - a^2 )^(1/2) -->
							<subfunction>
								<if>
									<lo><!-- 0 < r^2 - a^2 -->
										<value>0</value>
										<variable>5</variable>
									</lo>
									<!-- b = ( r^2 - a^2 )^(1/2) -->
									<exp>
										<variable>5</variable>
										<value>0.5</value>
									</exp>
									<!-- else b = 0 -->
									<value>0</value>
								</if>
							</subfunction>
							<list>
								<!-- upper half of circle -->
								<function define_variable="7">
									<!-- x_i = x + a -->
									<subfunction>
										<add>
											<variable>1</variable>
											<variable>4</variable>
										</add>
									</subfunction>
									<function define_variable="8">
										<!-- y_min = y - b -->
										<subfunction>
											<sub>
												<variable>2</variable>
												<variable>6</variable>
											</sub>
										</subfunction>
										<function define_variable="9">
											<!-- y_max = y + b -->
											<subfunction>
												<add>
													<variable>2</variable>
													<variable>6</variable>
												</add>
											</subfunction>
											<area define_variable="10">
												<vector type="subarea">
													<variable>8</variable>
													<variable>9</variable>
												</vector>
												<point>
													<vector type="position">
														<variable>7</variable>
														<variable>10</variable>
													</vector>
												</point>
											</area>
										</function>
									</function>
								</function>
								<!-- lower half of circle -->
								<function define_variable="7">
									<!-- x_i = x - a -->
									<subfunction>
										<sub>
											<variable>1</variable>
											<variable>4</variable>
										</sub>
									</subfunction>
									<function define_variable="8">
										<!-- y_min = y - b -->
										<subfunction>
											<sub>
												<variable>2</variable>
												<variable>6</variable>
											</sub>
										</subfunction>
										<function define_variable="9">
											<!-- y_max = y + b -->
											<subfunction>
												<add>
													<variable>2</variable>
													<variable>6</variable>
												</add>
											</subfunction>
											<area define_variable="10">
												<vector type="subarea">
													<variable>8</variable>
													<variable>9</variable>
												</vector>
												<point>
													<vector type="position">
														<variable>7</variable>
														<variable>10</variable>
													</vector>
												</point>
											</area>
										</function>
									</function>
								</function>
							</list>
						</function>
					</function>
				</area>
			</if>
		</main_fib_object>
	</root>
</fib_object>