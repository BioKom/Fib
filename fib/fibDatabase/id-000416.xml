														<subfunction>
															<round>
																<sub>
																	<variable>7</variable>
																	<value>0.5</value>
																</sub>
															</round>
														</subfunction>
														<if><!-- f_1( y ) < f_1( y + 1 ) -->
															<condition>
																<lo>
																	<variable>11</variable>
																	<variable>13</variable>
																</lo>
															</condition>
															<!-- fill area from max till min -->
															<function define_variable="16">
																<!-- max( f_1( y_min ), f_2( y_min ), min( f_1( ceil( y_min ) ), f_2( ceil( y_min ) ) ) ) -->
																<subfunction>
																	<max>
																		<max>
																			<variable>11</variable>
																			<variable>12</variable>
																		</max>
																		<min><!-- make connection to the part after this pixle line -->
																			<variable>13</variable>
																			<variable>14</variable>
																		</min>
																	</max>
																</subfunction>
																<function define_variable="17">
																	<!-- min( f_1( y_min ), f_2( y_min ) ) -->
																	<subfunction>
																		<min>
																			<variable>11</variable>
																			<variable>12</variable>
																		</min>
																	</subfunction>
																	<area define_variable="18">
																		<vector type="subarea">
																			<variable>16</variable>
																			<variable>17</variable>
																		</vector>
																		<subobject number="1">
																			<vector type="externSubobject">
																				<variable>18</variable>
																				<variable>15</variable>
																			</vector>
																		</subobject>
																	</area>
																</function>
															</function>
															<!-- else f_1( ceil( y_min ) ) =< f_1( y_min ) -->
															<function define_variable="16">
																<!-- max( f_1( y_min ), f_2( y_min ) ) -->
																<subfunction>
																	<max>
																		<variable>11</variable>
																		<variable>12</variable>
																	</max>
																</subfunction>
																<function define_variable="17">
																	<!-- min( f_2( y_min ), f_1( y_min ), max( f_1( ceil( y_min ) ), f_2( ceil( y_min ) ) ) ) -->
																	<subfunction>
																		<min>
																			<min>
																				<variable>11</variable>
																				<variable>12</variable>
																			</min>
																			<max><!-- make connection to the part after this pixle line -->
																				<variable>13</variable>
																				<variable>14</variable>
																			</max>
																		</min>
																	</subfunction>
																	<area define_variable="18">
																		<vector type="subarea">
																			<variable>16</variable>
																			<variable>17</variable>
																		</vector>
																		<subobject number="1">
																			<vector type="externSubobject">
																				<variable>18</variable>
																				<variable>15</variable>
																			</vector>
																		</subobject>
																	</area>
																</function>
															</function>
														</if>
													</function>
												</function>
											</function>
										</function>
									</function>
									<!-- for line at flour( y_max )
										go from max( f_1( y_max ), f_2( y_max ) )-->
									<function define_variable="11">
										<!-- f_1( flour( y_max ) ) = a_0 + a_1 * flour( y_max ) -->
										<subfunction>
											<add>
												<variable>3</variable>
												<mult>
													<variable>4</variable>
													<variable>10</variable>
												</mult>
											</add>
										</subfunction>
										<function define_variable="12">
											<!-- f_2( flour( y_max ) ) = b_0 + b_1 * flour( y_max ) -->
