													<variable>5</variable>
													<mult>
														<variable>6</variable>
														<variable>10</variable>
													</mult>
												</add>
											</subfunction>
											<function define_variable="13">
												<!-- f_1( y_max ) = a_0 + a_1 * y_max -->
												<subfunction>
													<add>
														<variable>3</variable>
														<mult>
															<variable>4</variable>
															<variable>8</variable>
														</mult>
													</add>
												</subfunction>
												<function define_variable="14">
													<!-- f_2( y_max ) = b_0 + b_1 * y_max -->
													<subfunction>
														<add>
															<variable>5</variable>
															<mult>
																<variable>6</variable>
																<variable>8</variable>
															</mult>
														</add>
													</subfunction>
													<if><!-- f_1( y ) < f_1( y + 1 ) -->
														<condition>
															<lo>
																<variable>11</variable>
																<variable>13</variable>
															</lo>
														</condition>
														<!-- fill area from max till min -->
														<function define_variable="15">
															<!-- max( f_1( flour( y_max ) ), f_2( flour( y_max ) ) , min( f_1( y_max ), f_2( y_max ) ) ) -->
															<subfunction>
																<max>
																	<max>
																		<variable>11</variable>
																		<variable>12</variable>
																	</max>
																	<min><!-- make connection to the part after this pixle line -->
																		<variable>13</variable>
																		<variable>14</variable>
																	</min>
																</max>
															</subfunction>
															<function define_variable="16">
																<!-- min( f_1( flour( y_max ) ), f_2( flour( y_max ) ) ) -->
																<subfunction>
																	<min>
																		<variable>11</variable>
																		<variable>12</variable>
																	</min>
																</subfunction>
																<area define_variable="17">
																	<vector type="subarea">
																		<variable>15</variable>
																		<variable>16</variable>
																	</vector>
																	<subobject number="1">
																		<vector type="externSubobject">
																			<variable>17</variable>
																			<variable>10</variable>
																		</vector>
																	</subobject>
																</area>
															</function>
														</function>
														<!-- else f_1( y_max ) =< f_1( flour( y_max ) ) -->
														<function define_variable="15">
															<!-- max( f_1( flour( y_max ) ), f_2( flour( y_max ) ) ) -->
															<subfunction>
																<max>
																	<variable>11</variable>
																	<variable>12</variable>
																</max>
															</subfunction>
															<function define_variable="16">
																<!-- min( f_1( flour( y_max ) ), f_2( flour( y_max ) ), max( f_2( y_max ), f_1( y_max ) ) ) -->
																<subfunction>
																	<min>
																		<min>
																			<variable>11</variable>
																			<variable>12</variable>
																		</min>
																		<max><!-- make connection to the part after this pixle line -->
																			<variable>13</variable>
																			<variable>14</variable>
																		</max>
																	</min>
																</subfunction>
																<area define_variable="17">
																	<vector type="subarea">
																		<variable>15</variable>
																		<variable>16</variable>
																	</vector>
																	<subobject number="1">
																		<vector type="externSubobject">
																			<variable>17</variable>
																			<variable>10</variable>
																		</vector>
																	</subobject>
																</area>
															</function>
														</function>
													</if>
												</function>
											</function>
										</function>
									</function>
								</list>
								
								<!-- else if flour( y_max ) < ceil( y_min ) go from ceil( y_min ) to flour( y_max ) -->
								<function define_variable="11">
									<!-- f_1( y_min ) = a_0 + a_1 * y_min -->
									<subfunction>
										<add>
											<variable>3</variable>
											<mult>
												<variable>4</variable>
												<variable>7</variable>
											</mult>
										</add>
									</subfunction>
									<function define_variable="12">
										<!-- f_2( y_min ) = b_0 + b_1 * y_min -->
										<subfunction>
											<add>
												<variable>5</variable>
												<mult>
													<variable>6</variable>
													<variable>7</variable>
												</mult>
											</add>
										</subfunction>
										<function define_variable="13">
											<!-- f_1( ceil( y_min ) ) = a_0 + a_1 * y_max -->
											<subfunction>
												<add>
													<variable>3</variable>
													<mult>
														<variable>4</variable>
