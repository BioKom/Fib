<?xml version="1.0" encoding="UTF-8"?>
<fib_object
			  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns="http://www.fib-development.org/"
			  xsd:schemaLocation="http://www.fib-development.org/fib.xsd">
	<root>
		<multimedia_info fib_version="3" db_version="2"/>
		<optionalpart>
			<pair key="name" value="vertical line"/>
			<pair key="description" value="This object represents a vertical line in two dimensions from an start point y_1 to an end point y_2 on a height x."/>
			<pair key="inVar1::description" value="position points in dimension 1 (x)"/>
			<pair key="inVar2::description" value="position start point dimension 2 (y_1)"/>
			<pair key="inVar3::description" value="position end point dimension 2 (y_2)"/>
			
			<pair key="author" value="Betti Oesterholz"/>
			<pair key="author::email" value="Oesterholz@Fib-Development.org"/>
			<pair key="copyright" value="LGPL3 (see database object with identifier -12)"/>
			
			<pair key="isPointElement" value=""/>
			<pair key="DbObject::isPointSubObject" value="-29"/>
			<pair key="isNotAntialiased" value=""/>
		</optionalpart>
		<input_variables>
			<variable number="1" default="0"/>
			<variable number="2" default="0"/>
			<variable number="3" default="0"/>
		</input_variables>
		<domains>
			<inVar number="1">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="2">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
			<inVar number="3">
				<domainReference subdomain="2">
					<dim count="2">
						<dimension number="1" direction="anywhere"/>
						<dimension number="2" direction="anywhere"/>
					</dim>
				</domainReference>
			</inVar>
		</domains>
		<main_fib_object>
			<area define_variable="4">
				<vector type="subarea">
					<variable>2</variable>
					<variable>3</variable>
				</vector>
				<point>
					<vector type="position">
						<variable>1</variable>
						<variable>4</variable>
					</vector>
				</point>
			</area>
		</main_fib_object>
	</root>
</fib_object>